<h1>All Jets For Sale</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Make</th>
      <th>Jet model</th>
      <th>Serial number</th>
      <th>Year</th>
      <th>Price</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @jets.each do |jet| %>
      <tr>
        <td><%= link_to jet.title, jet %></td>
        <td><%= jet.make %></td>
        <td><%= jet.jet_model %></td>
        <td><%= jet.serial_number %></td>
        <td><%= jet.year %></td>
        <td><%= jet.price %></td>
        <td><%= jet.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jet', new_jet_path %>
